// Fixed-point combinator (Y-combinator) for recursion
const Y = 位 recursive. (
    位 x. (x x)
    (位 self_apply. (recursive (self_apply self_apply))) // faster: node `(recursive (self_apply self_apply))` has 2 referenses now
)


const fib = (Y
    位 recursive n. (
        (> n 3)
            (+ (recursive (- n 1)) (recursive (- n 2)))
            1
    )
)
const main = (
    fib 20
    // 4181
)
